<h1>登録情報編集</h1>

<%= form_with model: @member, url: member_member_path(@member), local: true do |f| %>
	<div class="field">
    <%= f.label :姓 %><br />
    <%= f.text_field :first_name, autofocus: true, autocomplete: "first_name" %>
  </div>

  <div class="field">
    <%= f.label :名 %><br />
    <%= f.text_field :last_name, autofocus: true, autocomplete: "last_name" %>
  </div>

  <div class="field">
    <%= f.label :セイ %><br />
    <%= f.text_field :reading_first_name, autofocus: true, autocomplete: "reading_first_name" %>
  </div>

  <div class="field">
    <%= f.label :メイ %><br />
    <%= f.text_field :reading_last_name, autofocus: true, autocomplete: "reading_last_name" %>
  </div>

  <div class="field">
    <%= f.label :メールアドレス %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

	<div class="field">
    <%= f.label :郵便番号 %><br />
    <%= f.text_field :post_code, autofocus: true, autocomplete: "post_code" %>
  </div>

  <div class="field">
    <%= f.label :住所 %><br />
    <%= f.text_field :address, autofocus: true, autocomplete: "address" %>
  </div>

  <div class="field">
    <%= f.label :電話番号 %><br />
    <%= f.text_field :telephone_number, autofocus: true, autocomplete: "telephone_number" %>
  </div>

	<%= f.submit "編集内容を保存する" %>
<% end %>

<%= link_to "退会する", member_withdraw_path %>