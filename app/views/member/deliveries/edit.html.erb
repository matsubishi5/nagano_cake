<h1>配送先編集</h1>

<%= form_with model: @delivery, url: member_delivery_path(@delivery), local: true do |f| %>
	<%= f.label :郵便番号 %><br>
	<%= f.text_field :post_code %><br>

	<%= f.label :住所 %><br>
	<%= f.text_field :address %><br>

	<%= f.label :宛名 %><br>
	<%= f.text_field :name %><br>

	<%= f.submit "編集する" %>
<% end %>
