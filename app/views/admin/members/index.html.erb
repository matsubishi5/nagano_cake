<h1>会員一覧画面</h1>

<table>
	<thead>
		<tr>
			<th>会員ID</th>
			<th>氏名</th>
			<th>メールアドレス</th>
			<th>ステータス</th>
		</tr>
	</thead>

	<tbody>
		<% @members.each do |member| %>
			<tr>
				<td><%= member.id %></td>
				<td><%= link_to member.full_name, admin_member_path(member) %></td>
				<td><%= member.email %></td>
				<td><%= member.is_valid %></td>
			</tr>
		<% end %>
	</tbody>
</table>